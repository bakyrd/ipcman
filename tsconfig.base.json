{
  "extends": "@tsconfig/strictest/tsconfig",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "declaration": true,
    "composite": false,
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "noPropertyAccessFromIndexSignature": false,
  }
}
